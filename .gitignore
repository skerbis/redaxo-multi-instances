# REDAXO Multi-Instance Manager - GitIgnore

# Instanzen (enthalten sensitive Daten)
instances/*/
!instances/.gitkeep

# Penpot-Instanzen (enthalten sensitive Design-Daten)
penpot-instances/*/
!penpot-instances/.gitkeep

# SSL-Zertifikate und Keys (sensible Daten)
ssl/*/
!ssl/.gitkeep

# Backups
backups/*/
!backups/.gitkeep

# Penpot-Backups (sensible Design-Daten)
penpot-backups/*/
!penpot-backups/.gitkeep

# Downloads
downloads/*/
downloads/*.zip
!downloads/.gitkeep

# Dumps (enthalten sensitive Projektdaten)
dump/*/
dump/*.zip
!dump/.gitkeep

# Dashboard Screenshots (projektspezifische Daten)
dashboard/public/screenshots/*.png
dashboard/public/screenshots/*.jpg
dashboard/public/screenshots/*.jpeg
dashboard/public/screenshots/*.svg
dashboard/public/screenshots/*.html
!dashboard/public/screenshots/.gitkeep

# Logs
logs/*.log
logs/*/
!logs/.gitkeep

# Dashboard Logs (PM2 und Node.js Logs)
dashboard/logs/*.log
dashboard/logs/*/
!dashboard/logs/.gitkeep

# Temporary files
temp/*/
!temp/.gitkeep

# Docker volumes und builds
.docker/
docker-compose.override.yml

# Environment files
.env
.env.local
.env.production

# macOS
.DS_Store
.AppleDouble
.LSOverride

# IDE
.vscode/
.idea/
*.swp
*.swo

# Node modules (falls verwendet)
node_modules/

# Cache
*.cache
.cache/

# Runtime
*.pid
*.seed
*.pid.lock
